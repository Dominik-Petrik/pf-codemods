{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "title-heading-level": "error",
        "remove-variant-enum": "error"
    },
    "noInlineConfig": true,
    "reportUnusedDisableDirectives": false
}
